[926]
NAME="Variables locales - modification"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
TESTS=
  echo 'set var1=initial'
  echo 'set | grep var1'
  echo 'set var1=modified'
  echo 'set | grep var1'
[926-END]

[927]
NAME="Variables locales - unset"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
TESTS=
  echo 'set to_remove=value'
  echo 'set | grep to_remove'
  echo 'unset to_remove'
  echo 'set | grep to_remove'
[927-END]

[928]
NAME="Variables locales - multiple"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
