  echo 'set | grep var1'
  echo 'set | grep var2'
  echo 'set | grep var3'
[928-END]

[929]
NAME="Variables locales - unset multiples"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
TESTS=
