SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'ls -l -a'
  echo 'ls -lh'
[913-END]

[914]
NAME="Test ls with no path"
SETUP="export TERM=xterm ; PATH=''"
CLEAN=""
TESTS=
  echo 'ls -l src'
[914-END]

[915]
NAME="Test multiple commands"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'ls ; pwd'
[915-END]

[916]
NAME="Semicolon with cd and commands"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin' ; mkdir -p /tmp/test_dir"
CLEAN="rm -rf /tmp/test_dir"
TESTS=
  echo 'ls -l ; ls ; ls ; ls ; ls'
[916-END]

[917]
NAME="Simple Pipe command"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin' ; mkdir -p /tmp/test_dir"
CLEAN="rm -rf /tmp/test_dir"
TESTS=
  echo 'ls -l /dev | grep tty'
  echo 'ls -l; ls -l | wc -l'
[917-END]

[918]
NAME="Simple Redirection Out"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin' ; mkdir -p /tmp/test_dir"
CLEAN="rm -rf /tmp/test_dir ; rm a"
TESTS=
  echo 'ls -l /dev > a'
  echo 'cat a'
[918-END]

[919]
NAME="Hard Combination command"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin' ; mkdir -p /tmp/test_dir"
CLEAN="rm -rf /tmp/test_dir"
TESTS=
  echo 'mkdir test ; cd test ; ls -a ; ls | cat | wc -c > tutu ; cat tutu'
[919-END]

[920]
NAME="Simple Redirection In"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin' ; mkdir -p /tmp/test_dir"
CLEAN="rm -rf /tmp/test_dir ; rm file"
TESTS=
  echo 'ls > file'
  echo 'cat < file'
[920-END]

[921]
NAME="Test multiple pipe"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'ls -l | grep '.c' | wc -l'
[921-END]

[922]
NAME="Pipe and redirection"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN="rm c_files.txt"
TESTS=
  echo 'ls -l /home | grep ".c" > c_files.txt'
  echo 'cat c_files.txt'
[922-END]

[923]
NAME="*.c globbing"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
TESTS=
  echo 'ls -l ../src/*.c'
[923-END]

[924]
NAME="*many globbing"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
TESTS=
  echo 'ls -l ../src/*.c'
  echo 'ls -l ../src/*e'
  echo 'ls -l *e'
  echo 'ls *e'
  echo 'ls *e'
  echo 'ls *e'
  echo 'ls *e'
[924-END]

